{
  "expo": {
    "name": "MOV",
    "slug": "mov",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mov",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/icon/bolt-mov-icon-ios.png",
      "statusBar": "light-content",
      "supportsTablet": false,
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) uses your camera to record videos at events and capture live moments.",
        "NSMicrophoneUsageDescription": "$(PRODUCT_NAME) uses your microphone to record audio while shooting videos during events.",
        "NSPhotoLibraryUsageDescription": "$(PRODUCT_NAME) needs access to your photo library so you can select photos for profiles and event sharing.",
        "NSContactsUsageDescription": "$(PRODUCT_NAME) uses your contacts to help you find and invite friends to events and share your videos with them.",
        "NSLocationWhenInUseUsageDescription": "$(PRODUCT_NAME) doesn't use your location.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "production",
        "com.apple.developer.applesignin": ["Default"]
      },
      "bundleIdentifier": "com.alexisbouley2.mov",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "icon": "./assets/images/icon/bolt-mov-icon-android.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon/bolt-mov-icon-android.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_CONTACTS",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE"
      ],
      "package": "com.alexisbouley2.mov",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      },
      "navigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content",
        "visible": true
      },
      "userInterfaceStyle": "dark",
      "googleServicesFile": "./google-services.json",
      "config": {
        "largeHeap": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/mov.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/splash-mov-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) uses your camera to record videos at events and capture live moments.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(PRODUCT_NAME) uses your microphone to record audio while shooting videos during events."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME) needs access to your photo library so you can select photos for profiles and event sharing."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "$(PRODUCT_NAME) needs access to your photo library to save videos.",
          "savePhotosPermission": "$(PRODUCT_NAME) needs access to your photo library to save videos.",
          "isAccessMediaLocationEnabled": false
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "(PRODUCT_NAME) uses your contacts to help you find and invite friends to events and share your videos with them."
        }
      ],
      "expo-web-browser",
      "@react-native-firebase/app",
      [
        "@react-native-firebase/messaging",
        {
          "enableBackgroundSync": true,
          "android": {
            "headlessTaskTimeout": 60000,
            "channelId": "mov-notifications",
            "channelName": "MOV Notifications",
            "channelDescription": "Notifications for MOV events and messages"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.894319536992-66f09eb8cdeljrlsd5sb3sspgth0r87q"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a28046e5-d979-4847-aa7a-d9b2fac9e8b7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a28046e5-d979-4847-aa7a-d9b2fac9e8b7"
    }
  }
}
