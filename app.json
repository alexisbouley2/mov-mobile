{
  "expo": {
    "name": "MOV",
    "slug": "mov",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mov",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/icon/bolt-mov-icon-ios.png",
      "statusBar": "light-content",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "$(PRODUCT_NAME) needs access to your Camera.",
        "NSMicrophoneUsageDescription": "$(PRODUCT_NAME) needs access to your Microphone.",
        "NSPhotoLibraryUsageDescription": "$(PRODUCT_NAME) needs access to your Photo Library to select photos for your profile and events.",
        "NSContactsUsageDescription": "$(PRODUCT_NAME) needs access to your Contacts to help you invite friends to events.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "bundleIdentifier": "com.alexisbouley2.mov",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "icon": "./assets/images/icon/bolt-mov-icon-android.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon/bolt-mov-icon-android.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_CONTACTS",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE"
      ],
      "package": "com.alexisbouley2.mov",
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      },
      "navigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content",
        "visible": true
      },
      "userInterfaceStyle": "dark",
      "googleServicesFile": "./google-services.json",
      "config": {
        "largeHeap": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/mov.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/splash-mov-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(PRODUCT_NAME) needs access to your Microphone."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your Photo Library to select photos for your profile and events."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow access to your Photo Library to save videos.",
          "savePhotosPermission": "Allow access to your Photo Library to save videos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow access to your Contacts to help you invite friends to events."
        }
      ],
      "expo-web-browser",
      "@react-native-firebase/app",
      [
        "@react-native-firebase/messaging",
        {
          "enableBackgroundSync": true,
          "android": {
            "headlessTaskTimeout": 60000,
            "channelId": "mov-notifications",
            "channelName": "MOV Notifications",
            "channelDescription": "Notifications for MOV events and messages"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a28046e5-d979-4847-aa7a-d9b2fac9e8b7"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a28046e5-d979-4847-aa7a-d9b2fac9e8b7"
    }
  }
}
